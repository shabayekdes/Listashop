{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "cartalyst/stripe-laravel": "^11.0",
        "intervention/image": "^2.5",
        "laravel/passport": "^7.4",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "facade/ignition": "^1.11",
        "laravel/ui": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Shop\\": "packages/Shop/src",
            "Category\\": "packages/Category/src",
            "Product\\": "packages/Product/src",
            "Admin\\": "packages/Admin/src",
            "User\\": "packages/User/src",
            "Core\\": "packages/Core/src",
            "Cart\\": "packages/Cart/src",
            "Order\\": "packages/Order/src",
            "Attribute\\": "packages/Attribute/src",
            "Option\\": "packages/Option/src",
            "Value\\": "packages/Value/src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "packages/Core/src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
